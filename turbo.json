{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".output/**", "dist/**", "genql/**"],
			"env": ["NODE_ENV"]
		},
		"dev": {
			"dependsOn": ["@politigraph/graphql#build", "migrate"],
			"persistent": true,
			"cache": false
		},
		"migrate": {
			"cache": false
		}
	}
}
